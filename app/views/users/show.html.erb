

<%= link_to current_user.spotify_id, user_path %>
<hr>
<%= link_to @user.spotify_id, user_path %>
<hr>
<% @playlists[:items].each do |playlist| %>
<div>
  <%= image_tag playlist[:images][0][:url], size: "80" %>

  <% if @tracks[playlist[:id]] %>
    <% @tracks[playlist[:id]].each do |track| %>
    <div>
      <%= track[:track][:name] %>
    </div>
    <% end %>

  <% end %>
</div>
<% end %>

<pre>
<%= JSON.pretty_generate @playlists %>
</pre>
